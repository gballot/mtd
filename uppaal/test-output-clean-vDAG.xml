<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>const int n_a = 2;
const int n_d = 1;
hybrid clock time;
hybrid clock cost;

hybrid clock xcost;

hybrid clock x_a_0, x_a_1;
const int t_a_0 = 30, t_a_1 = 10;
//const int p_a_0 = 1, p_a_1 = 0.5;
const int c_a_0 = 50, c_a_1 = 0;
const int cp_a_0 = 0, cp_a_1 = 1;
hybrid clock x_d_0;
const int t_d_0 = 40;
//const int p_d_0 = 1;

int prob =10;</declaration>
	<template>
		<name x="0" y="0">AttackDefenseADMDP</name>
		<location id="id0" x="-544" y="374">
			<name x="-594" y="340">____NORMAL</name>
			<label kind="invariant" x="-594" y="394">cost' == 0 &amp;&amp;
x_d_0 &lt;= t_d_0</label>
		</location>
		<location id="id1" x="-544" y="170">
			<name x="-594" y="136">____ACTIVATION_COST__a_0</name>
			<label kind="invariant" x="-594" y="190">time' == 0 &amp;&amp;
xcost &lt;= 1 &amp;&amp;
cost' == c_a_0 &amp;&amp;
x_a_0' == 0 &amp;&amp;
x_a_1' == 0 &amp;&amp;
x_d_0' == 0</label>
		</location>
		<location id="id2" x="-340" y="170">
			<name x="-357" y="136">a_0____NORMAL</name>
			<label kind="invariant" x="-390" y="190">cost' == cp_a_0 &amp;&amp;
x_d_0 &lt;= t_d_0 &amp;&amp;
x_a_0 &lt;= t_a_0</label>
		</location>
		<location id="id3" x="68" y="374">
			<name x="85" y="348">__g_0__NORMAL</name>
			<label kind="invariant" x="76" y="382">time' == 0 &amp;&amp;
cost' == 0</label>
		</location>
		<location id="id4" x="-136" y="170">
			<name x="-186" y="136">a_0____NO_ACTIVATION</name>
			<label kind="invariant" x="-186" y="190">cost' == cp_a_0 &amp;&amp;
x_d_0 &lt;= t_d_0 &amp;&amp;
x_a_0 &lt;= t_a_0</label>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
			<label kind="assignment" x="-544" y="297">xcost = 0</label>
		</transition>
		<transition controllable="false">
			<source ref="id1"/>
			<target ref="id2"/>
			<label kind="guard" x="-493" y="170">xcost &gt;= 1</label>
			<label kind="assignment" x="-493" y="153">x_a_0 = 0</label>
		</transition>
		<transition>
			<source ref="id2"/>
			<target ref="id4"/>
		</transition>
		<transition controllable="false">
			<source ref="id4"/>
			<target ref="id3"/>
			<label kind="guard" x="-85" y="170">x_a_0 &gt;= t_a_0</label>
			<nail x="68" y="170"/>
		</transition>
	</template>
	<system>system AttackDefenseADMDP;</system>
	<queries>
		<query>
			<formula>strategy safe = control: A&lt;&gt; AttackDefenseADMDP.__g_0__NORMAL</formula>
			<comment></comment>
		</query>
		<query>
			<formula>strategy cheap = minE(cost)[time&lt;=10]: &lt;&gt;AttackDefenseADMDP.__g_0__NORMAL</formula>
			<comment>Cheap strategy</comment>
		</query>
		<query>
			<formula>E[time&lt;=10000;10000](max: time) under cheap</formula>
			<comment>Expected time under cheap</comment>
		</query>
		<query>
			<formula>E[time&lt;=10000;10000](max: cost) under cheap</formula>
			<comment>Expected cost under cheap</comment>
		</query>
		<query>
			<formula>Pr[time&lt;=10](&lt;&gt;AttackDefenseADMDP.__g_0__NORMAL) under cheap</formula>
			<comment>Success probability under cheap</comment>
		</query>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
