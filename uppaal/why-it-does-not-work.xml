<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
hybrid clock time;</declaration>
	<template>
		<name x="5" y="5">Template</name>
		<declaration>// Place local declarations here.</declaration>
		<location id="id0" x="-161" y="-93">
			<name x="-171" y="-127">A</name>
			<urgent/>
		</location>
		<location id="id1" x="8" y="-93">
			<name x="-2" y="-127">C</name>
		</location>
		<location id="id2" x="-76" y="-25">
			<name x="-86" y="-59">B</name>
		</location>
		<location id="id3" x="85" y="-153">
			<name x="75" y="-187">D</name>
			<label kind="invariant" x="93" y="-187">time'==0</label>
		</location>
		<location id="id4" x="119" y="-102">
			<name x="109" y="-136">E</name>
		</location>
		<init ref="id0"/>
		<transition>
			<source ref="id1"/>
			<target ref="id4"/>
		</transition>
		<transition>
			<source ref="id1"/>
			<target ref="id3"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id1"/>
		</transition>
	</template>
	<system>// Place template instantiations here.
Process = Template();
// List one or more processes to be composed into a system.
system Process;
    </system>
	<queries>
		<query>
			<formula>strategy goCmin = minE (time) [&lt;=200] : &lt;&gt;Process.D</formula>
			<comment></comment>
		</query>
		<query>
			<formula>//Pr[&lt;=200](&lt;&gt;Process.D)</formula>
			<comment></comment>
		</query>
		<query>
			<formula>Pr[&lt;=200](&lt;&gt;Process.D) under goCmin</formula>
			<comment></comment>
		</query>
		<query>
			<formula>E[&lt;=200;500] (max:time) under goCmin</formula>
			<comment></comment>
		</query>
		<query>
			<formula>strategy goC = control: A&lt;&gt; Process.D</formula>
			<comment></comment>
		</query>
		<query>
			<formula>Pr[&lt;=200](&lt;&gt;Process.D) under goC</formula>
			<comment></comment>
		</query>
		<query>
			<formula>E[&lt;=200;500] (max:time) under goC</formula>
			<comment></comment>
		</query>
		<query>
			<formula>E&lt;&gt; control: A&lt;&gt;Process.D</formula>
			<comment></comment>
		</query>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
